
#include <iostream>
#include <string>
using namespace std;

int hamming_distance(string a, string b)
{
    int count = 0;
    for(int i = 0; i < a.length(); ++i)
{
        if(a[i] != b[i])
        {
            count++;
        }


    }
    return count;


}
int main()
{
    string a, b;
    cin >> a >> b;

    cout << hamming_distance(a, b);




    return 0;
}

